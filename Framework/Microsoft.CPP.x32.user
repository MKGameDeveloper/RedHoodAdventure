<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <DH>C:\Program Files (x86)\Microsoft DirectX SDK (June 2010)\Include</DH>
    <DL>C:\Program Files (x86)\Microsoft DirectX SDK (June 2010)\Lib\x86</DL>
  </PropertyGroup>
  <PropertyGroup>
    <_PropertySheetDisplayName>Microsoft.CPP.x86.user</_PropertySheetDisplayName>
    <IntDir>$(SolutionDir)$(Configuration)\</IntDir>
    <IncludePath>$(VC_IncludePath);$(WindowsSDK_IncludePath);$(DH);</IncludePath>
    <LibraryPath>$(VC_LibraryPath_x86);$(WindowsSDK_LibraryPath_x86);$(DL);</LibraryPath>
  </PropertyGroup>
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="DH">
      <Value>$(DH)</Value>
    </BuildMacro>
    <BuildMacro Include="DL">
      <Value>$(DL)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>